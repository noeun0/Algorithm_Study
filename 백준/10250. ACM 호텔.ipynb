    "# 층수, 방수 , 손님 번수\n",
    "for tc in range(1, int(input())+1):\n",
    "    H,W,N = map(int,input().split())\n",
    "    result =0\n",
    "    num = N//H\n",
    "    fl = N%H\n",
    "    if fl == 0:\n",
    "        fl=H\n",
    "        num-=1\n",
    "    result = fl*100+num+1\n",
    "        \n",
    "    print(result)\n",
    "    "
